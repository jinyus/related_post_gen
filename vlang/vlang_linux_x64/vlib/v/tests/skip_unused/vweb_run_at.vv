import vweb
import time

struct App {
	vweb.Context
}

fn main() {
	spawn fn () {
		time.sleep(100 * time.millisecond)
		println('done')
		exit(0)
	}()
	vweb.run_at(&App{}, port: 38090, nr_workers: 1)!
	//	vweb.run(&App{}, 38091)
}

['/']
pub fn (mut app App) app_main() vweb.Result {
	return app.text('Hello World')
}
