fn test_add() {
	println('test_add()')
	x := 2
	y := 3
	sum := x + y
	product := x * y
	diff := y - x
	div := y / x
	mod := y % x
	bitand := x & y
	bitor := x | y
	bitxor := x ^ y
	lshift := x << x
	rshift := 12 >> x
	print_number(x)
	print_number(y)
	print_number(sum)
	print_number(product)
	print_number(diff)
	print_number(div)
	print_number(mod)
	print_number(bitand)
	print_number(bitor)
	print_number(bitxor)
	print_number(lshift)
	print_number(rshift)
}

fn test_multiple() {
	a := 3
	b := 5
	c := 2
	result := (a * ((a & b) + c) << (b ^ a)) | a - b % (b / c)
	println(result)
}

fn test_prefix() {
	a := 3
	assert -a == -3
	b := true
	assert !b == false
	assert !!b == true
	c := u8(73)
	assert ~c == 182
}

fn main() {
	println('start')
	test_add()
	test_multiple()
	println('end')
}
